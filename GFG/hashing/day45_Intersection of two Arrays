class Solution {
  public:
    vector<int> intersect(vector<int>& a, vector<int>& b) {
        // code here
        unordered_set<int>s(a.begin(),a.end());
        vector<int>res;
        
        for(int i=0;i<b.size();i++){
            
            if(s.find(b[i])!=s.end()){
                res.push_back(b[i]);
                s.erase(b[i]);
            }
        }
        return res;
    }
};
