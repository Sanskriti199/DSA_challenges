class Solution {
  public:
    vector<int> findUnion(vector<int>& a, vector<int>& b) {
        // code here
        unordered_set<int>s;
        
        for(int i=0;i<a.size();i++)
        s.insert(a[i]);
        
        for(int i=0;i<b.size();i++)
        s.insert(b[i]);
        
        vector<int>res;
        
        for(auto it:s)
        res.push_back(it);
        
        return res;
    }
};
