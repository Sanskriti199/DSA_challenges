class Solution {
  public:
    int missingNumber(vector<int> &arr) {
        // code here
        int a=arr.size();
        
        for(int i=0;i<a;i++){
            
            while(arr[i]>=1 && arr[i]<=a && arr[i]!=arr[arr[i]-1]){
                
                swap(arr[i],arr[arr[i]-1]);
            }
        }
        for(int i=1;i<=a;i++){
            if(i!=arr[i-1]){
            return i;
            }
        }
        return a+1;
    }
};
