class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
        int a = nums.size();
        vector<int> answer(a);

        int pp= 1;
        for (int i = 0; i < a; ++i) {
            answer[i] = pp;
            pp *= nums[i];
        }
        int sp= 1;
        for (int i = a- 1; i >= 0; --i) {
            answer[i] *= sp;
            sp *= nums[i];
        }

        return answer;
    }
};
