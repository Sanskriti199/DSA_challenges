class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int a=nums.size();
        if (a <= 2) {
            return a;
        }
        int i = 2;
    
        for (int j = 2; j < a; ++j) {
            if (nums[j] != nums[i - 2]) {
                nums[i] = nums[j];
                i++;
            }
        }
        return i;
    }
};
